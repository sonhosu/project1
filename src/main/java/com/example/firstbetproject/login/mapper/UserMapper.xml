<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.firstbetproject.login.dao.UserDao">
	
	<insert id="insert" >
		insert into user_info
			(user_id, user_pw,user_tel,user_email,user_role)
		values
		(#{userid}, #{userpw}, #{usertel},#{useremail},#{userrole})
	 	
	</insert>

</mapper>